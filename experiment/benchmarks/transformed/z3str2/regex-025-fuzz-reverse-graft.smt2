(declare-const x String)
(declare-const y String)
(assert (str.in.re x (re.* (re.union (re.* (str.to.re "a")) (str.to.re "b")))))
(assert (str.in.re y (re.+ (re.union (str.to.re "b") (re.* (str.to.re "a"))))))
(assert (= (str.len x) 0))
(assert (not (= x y)))
(assert (= (str.len x) (str.len y)))
(check-sat)
(get-model)
